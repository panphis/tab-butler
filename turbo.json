{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "tsc": {
      "dependsOn": [
        "^tsc"
      ]
    },
    "build": {
      "dependsOn": [
        "tsc",
        "^build"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env*"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        "dist/**"
      ]
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}