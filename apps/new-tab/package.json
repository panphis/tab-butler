{
	"name": "@repo/new-tab",
	"version": "0.2.2",
	"description": "chrome extension new tab",
	"private": true,
	"scripts": {
		"clean": "rimraf ./dist",
		"build": "pnpm run clean&& pnpm type-check && vite build",
		"build:watch": "cross-env __DEV__=true vite build --mode development",
		"dev": "pnpm build:watch",
		"lint": "eslint . --ext .ts,.tsx",
		"lint:fix": "pnpm lint --fix",
		"prettier": "prettier . --write --ignore-path ../../.prettierignore",
		"type-check": "tsc --noEmit"
	},
	"dependencies": {
		"@repo/shared": "workspace:*",
		"@repo/storage": "workspace:*",
		"@repo/theme": "workspace:*",
		"@repo/ui": "workspace:*"
	},
	"devDependencies": {
		"@repo/tailwindcss-config": "workspace:*",
		"@repo/typescript-config": "workspace:*",
		"@repo/vite-config": "workspace:*",
		"@types/react": "^18.2.61",
		"@types/react-dom": "^18.2.19",
		"sass": "1.77.8",
		"vite": "^5.4.0"
	}
}